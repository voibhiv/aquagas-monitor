{
  "name": "aquagas-monitor",
  "version": "1.0.0",
  "description": "An water and gas tip consumption tracker",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only 'src/index.ts'",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint ./src ./test --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "javascript",
    "api"
  ],
  "author": "Leandro Melo",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@overnightjs/core": "^1.7.6",
    "@prisma/client": "^5.19.0",
    "axios": "^1.7.5",
    "body-parser": "^1.20.2",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "prisma": "^5.19.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
